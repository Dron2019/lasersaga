  script(defer src='./assets/scripts/index.bundle.js')
  script(defer src='./assets/scripts/libs.js')
  script(defer src='./assets/scripts/header.js')
  script(defer src='./assets/scripts/menu.js')
  script(defer src='./assets/scripts/sliderCursor.js')
  script(defer src='./assets/scripts/services.js')
  script(defer src='./assets/scripts/footer.js')
block content
  .container-page-header
    .horizontal-red
    .vertical-red-left
    .page-header
      .noise
      .page-header-content
        ul.breadcrumbs
          li.breadcrumbs-item
            a(href="https:main.html")
              span Home
          li.breadcrumbs-item.breadcrumbs-item-active
            a(href="https:services.html")
              span Services
        h2(data-text="Services").page-title
          span Services
    .vertical-red-right
    .horizontal-red-bottom

  .equipment
    .equipment-title
      h4 reliable equipment
    .equipment-description
      p At LSP we believe in quality, therefor we only use reliable equipment at our shows and installations. Laser Saga Productions is representing and supporting KVANT Laser, Pangolin Laser Systems, TechArt Laser in UAE and Middle East.
    .equipment-img
      img(src="./assets/images/equipment.jpg", alt="")

